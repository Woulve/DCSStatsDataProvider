{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Uvicorn",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload",
            ],
            "cwd": "./DCSStatsDataProvider/",
            "env": {
                "PYTHONUNBUFFERED": "1"
            },
            "justMyCode": true,
            "pythonArgs": ["-Xfrozen_modules=off"]
        }
    ]
}
